<script language="javascript">
    $(document).ready(function () {
        $(".header .select-bar").bind("click", function (event) {
            $(".pop-box").slideToggle();
            event.stopPropagation();
        });
        $(".pop-box").bind("touchstart click", function (event) {
            event.stopPropagation();
        })
        $(document).bind("touchstart click", function () {
            var isVisible = $('.pop-box').css('display');
            if (isVisible !== 'none') {
                $('.pop-box').slideUp();
            }
        });
    });
</script>
